        <arguments>
          <argument>
            <kind>1</kind>
            <string>//control init
key_left = -keyboard_check(vk_left);
key_right = keyboard_check(vk_right);
key_jump = keyboard_check_pressed(vk_space);
key_up = keyboard_check_pressed(vk_up);
key_down = keyboard_check_pressed(vk_down);
//init gravity
if vspd &lt; max_grav
    {
        vspd += grav;
    }
//Jumping and updating horizontal speed   
if place_meeting(x, y + 1, obj_wall)
    {
        vspd = key_jump * -jump_speed
    }
    
move = key_left + key_right;
hspd = move * move_speed;

//update player facing based on keyboard inputs
if key_left = -1
    {
        p1_facing = p1_left;
    }
else if key_right = 1
    {
        p1_facing = p1_right;
    }

//horizontal collision
if place_meeting(x + hspd, y, obj_wall)
    {
        if !place_meeting(x + sign(hspd), y, obj_wall)
            {
                x += sign(hspd);
            }
        hspd = 0;
    }
//vertical collision
if place_meeting(x, y + vspd, obj_wall)
    {
        if !place_meeting(x, y + sign(vspd), obj_wall)
            {
                y += sign(vspd);
            }
        vspd = 0;
    }
/*wrap the screen when the character goes outside the room
if x &gt; room_width
    {
        x = 0;
    }
if x &gt; 0
    {
        x = 1000;
    }*/
    
//Updating the objects position on the map    
x += hspd;
y += vspd;

if place_meeting (x + hspd, y, obj_part1)
    {
        obj_controller.parts_collected += 1;
        with (instance_nearest(x, y, obj_part1))
            {
                instance_destroy();
            }
        obj_controller.alarm[0] = 30;
    }
//Elevator INTERaction    
if place_meeting(x, y, obj_elevator)
    {
        if key_up = 1 &amp;&amp; current_floor = 1
            {
                y = obj_controller.floors[1];
                current_floor += 1;
            }
        else if key_down = 1 &amp;&amp; current_floor = 2
            {
                y = obj_controller.floors[2];
                current_floor -=1;
            }
        else if key_up = 1 &amp;&amp; current_floor = 2
            {
                y = obj_controller.floors[0];
                current_floor += 1;
            }
        else if key_down = 1 &amp;&amp; current_floor = 3
            {
                y = obj_controller.floors[1];
                current_floor -=1;
            }
    }
</string>
